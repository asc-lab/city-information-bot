{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": [false],
        "no-console": [false, "log", "error"],
        "quotemark": [true, "single"],
        "ordered-imports": false,
        "trailing-comma": false,
        "variable-name": [true, "check-format", "allow-leading-underscore"]
    },
    "rulesDirectory": []
}
